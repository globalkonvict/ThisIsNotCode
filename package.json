{
  "name": "thisisnotcode",
  "version": "0.1.0",
  "description": "TINC(aka This Is Not Code) is an optimaly designed to start a Node+React App.",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-start": "npm start --prefix client",
    "client-css-start": "npm run css-watch --prefix client",
    "dev": "concurrently \"npm run client-start \" \"npm run client-css-start \" \"npm run server\""
  },
  "keywords": [
    "Mern",
    "App",
    "React",
    "Node"
  ],
  "author": "Sarthak Dwivedi",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.4.1"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1"
  }
}
